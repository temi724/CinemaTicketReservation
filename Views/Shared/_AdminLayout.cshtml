
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/DataTables (1)/DataTables-1.10.20/css/jquery.dataTables.css" rel="stylesheet" />
    <script src="~/Content/DataTables (1)/jQuery-3.3.1/jquery-3.3.1.min.js"></script>
    <script src="~/Content/DataTables (1)/DataTables-1.10.20/js/jquery.dataTables.min.js"></script>
    <link href="~/Content/DataTables (1)/Buttons-1.6.1/css/buttons.dataTables.css" rel="stylesheet" />
    <script src="~/Content/DataTables (1)/Buttons-1.6.1/js/dataTables.buttons.js"></script>
    <script src="~/Content/DataTables (1)/JSZip-2.5.0/jszip.js"></script>
    <script src="~/Content/DataTables (1)/pdfmake-0.1.36/pdfmake.min.js"></script>
    <script src="~/Content/DataTables (1)/pdfmake-0.1.36/vfs_fonts.js"></script>

</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <a class="navbar-brand" href="#">Admin</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
                </li>
              
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        @Session["User"]
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a class="dropdown-item" href="@Url.Action("Logout","Account")">Logout</a>
                        

                    </div>
                </li>

            </ul>



        </div>
    </nav>
    <div class="">
        @RenderBody()

        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @*@Scripts.Render("~/bundles/jquery")*@
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
